{
  "$uri" : {
    "get" : "contient"
  },
  "$format" : {
    "set" : "jsonld"
  },
  "$params" : {
    "inject" : {
      "alt<" : {
        "get" : "format"
      }
    },
    "querystring" : true
  },
  "$records" : {
    "get" : ["uri","params"],
    "join": "?",
    "fetch": true,
    "parseJSON": true
  }
}
